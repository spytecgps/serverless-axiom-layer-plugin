{
  "name": "@spytecgps/serverless-axiom-layer-plugin",
  "version": "0.0.20",
  "description": "This plugin will add Axiom layer to your lambda functions",
  "main": "dist/index.js",
  "private": false,
  "homepage": "https://gethapn.com",
  "scripts": {
    "lint": "eslint --cache --cache-strategy content .",
    "lint:fix": "eslint --cache --cache-strategy content . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --ignore-path .gitignore --write . !**.md",
    "format-check": "prettier --ignore-path .gitignore --check . !**.md",
    "prepare": "npm run build",
    "build": "tsc"
  },
  "keywords": [
    "serverless",
    "serverless-plugin",
    "axiom-layer-extensions",
    "logs"
  ],
  "author": "Hapn",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.0.0",
    "@types/serverless": "^1.78.34",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "prettier": "^3.0.2",
    "serverless": "^3.34.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.7.0"
  },
  "files": [
    "dist"
  ]
}
