{
  "name": "@spytecgps/serverless-axiom-layer-plugin",
  "version": "0.0.15",
  "description": "This plugin will add Axiom layer to your lambda functions",
  "main": "dist/index.js",
  "private": false,
  "homepage": "https://gethapn.com",
  "scripts": {
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "format": "prettier --ignore-path .gitignore --write . !**.md",
    "format-check": "prettier --ignore-path .gitignore --check . !**.md",
    "build": "tsc",
    "publish-prod": "tsc && npm publish"
  },
  "keywords": [
    "serverless",
    "serverless-plugin",
    "axiom-layer-extensions",
    "logs"
  ],
  "author": "Hapn",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.5.6",
    "@types/serverless": "^1.78.34",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.2",
    "serverless": "^3.34.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist"
  ]
}
